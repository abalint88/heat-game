{
	"name": "parcel-boilerplate",
	"version": "1.0.0",
	"description": "Simple starter pack",
	"main": "index.js",
	"scripts": {
		"format": "prettier --write \"src/**/*.{js,css,scss,json} \"",
		"format:check": "prettier --list-different \"src/**/*.{js,css,scss,json} \"",
		"lint": "eslint \"src/*.{js}\"",
		"lint:fix": "eslint --fix \"src/*.{js}\"",
		"dev": "parcel src/index.html --no-cache",
		"build": "parcel build src/index.html -d ../purejs-dist --public-url ./"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/zeracoke/react-jam.git"
	},
	"keywords": [],
	"author": "Balint Apro",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^9.3.1",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.0.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-latest": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"eslint": "^5.7.0",
		"eslint-config-prettier": "^3.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-prettier": "^3.0.0",
		"mapbox-gl": "^0.53.1",
		"parcel-bundler": "^1.10.3",
		"prettier": "^1.14.3",
		"sass": "^1.14.3"
	},
	"dependencies": {
		"config.js": "^0.1.0",
		"fetchers": "^0.2.1"
	}
}
